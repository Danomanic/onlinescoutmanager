{
  "name": "osm-api",
  "version": "1.0.0",
  "description": "Modern TypeScript client for the Online Scout Manager (OSM) API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "check": "biome check src/ test/",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danomanic/osm-node.git"
  },
  "keywords": [
    "osm",
    "online-scout-manager",
    "scouts",
    "api"
  ],
  "author": "Daniel J. Pomfret",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Danomanic/osm-node/issues"
  },
  "homepage": "https://github.com/Danomanic/osm-node#readme",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^3.0.0"
  }
}
